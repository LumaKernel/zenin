{
  "name": "@template/domain",
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "description": "The domain template",
  "repository": {
    "type": "git",
    "url": "<PLACEHOLDER>",
    "directory": "pkg/domain"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "scripts": {
    "codegen": "build-utils prepare-v2",
    "build": "npm run build-esm",
    "check": "tsc -b tsconfig.json",
    "test": "vitest",
    "coverage": "vitest --coverage"
  },
  "dependencies": {
    "@effect/platform": "latest",
    "@effect/sql": "latest",
    "effect": "latest"
  },
  "effect": {
    "generateExports": {
      "include": [
        "**/*.ts"
      ]
    },
    "generateIndex": {
      "include": [
        "**/*.ts"
      ]
    }
  }
}
